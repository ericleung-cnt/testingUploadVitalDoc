<DataSource ID="crewDS" serverType="generic" inheritsFrom="abstractPersistentEntityDS">
	<serverObject lookupStyle="spring" bean="crewDMI" />
	<fields>
		<field name="id" 		type="integer" 	primaryKey="true"/>
		<field name="referenceNo" 	type="text" title="Reference NO."  required="true" length="50"/>
		<field name="imoNo" 	type="text" title="IMO No."   length="9"/>
		<field name="crewName" 	type="text" 	title="Crew Name" 	length="50" required="true"/>
		<field name="sex" 	type="text" 	title="Sex" 	length="10">
			<valueMap>
			   	<value ID="M">Male</value>
			   	<value ID="F">Female</value>
	   		</valueMap>
		</field>
		<field name="serbNo" 		type="text" 	title="SERB NO."	required="true"	length="15"/>
		<field name="nationalitybeforeMap" 		type="text" 	length="50"/>
		<field name="nationalityId" type="integer" 	title="Nationality ID" 	required="true"/>
		<field name="birthDate" 	type="date" 	title="Birth Date"		displayFormat="dd/MM/yyyy"/>
		<field name="birthPlace" 	type="text" 	title="Birth Place"		length="50"/>
		<field name="address" 		type="text" 	title="Address "		length="250"/>

		<field name="nokName" 		type="text" 	title="Name of Kin"		length="50"/>
		<field name="nokAddress" 		type="text" 	title="Relationship of Kin &amp; Address(if any) "		length="250"/>
		<field name="capacityBeforeMap" 		type="text" 	length="50"/>
		<field name="capacityId" 	type="integer" 	title="Capacity ID" 	required="true"/>
		<field name="crewCert" 		type="text" 	title="Crew Cert"		length="50"/>
		<field name="currency" 		type="text" 	title="Currency"		length="5" required="true">
			<!-- <valueMap>
			   		<value ID="HKD">HKD</value>
			   		<value ID="CNY">CNY</value>
					<value ID="USD">USD</value>
					<value ID="BGP">BGP</value>
			</valueMap> -->
		</field>
		<field name="salary" 		type="decimal" 	title="Salary" 	 format="$,##0.00"	required="true"/>
		<field name="engageDate" 	type="date" displayFormat="dd/MM/yyyy" 	title="Engagement Date"		/>
		<field name="engagePlace" 	type="text" 	title="Engage Place"	length="50"/>
		<field name="dischargeDate"	type="date" displayFormat="dd/MM/yyyy" 	title="Discharge Date"/>
		<field name="dischargePlace" 	type="text" title="Discharge Place"	length="50"/>
		<field name="employDate"	type="date" displayFormat="dd/MM/yyyy" 	title="Commencement Date(SEA)"/>
		<field name="employDuration" 	type="decimal" title="SEA Duration(Months)"	/>
		<field name="status" 		type="text" 	title="Status"			length="1">
				<valueMap>
				<value ID="A">Active</value>
			   	<value ID="I">Inactive</value>
			   	<value ID="D">Deceased</value>
			</valueMap>
		</field>
		<!-- <field name="rSalary" 		type="integer" 	title="R Salary"		/> -->
		<field name="crewListCover" type="crewListCoverDS"/>
		<field name="nationality" 	type="any"/>
		<field name="capacity" 			type="rankDS"/>
		<field name="validationErrors" 			type="text"/>
		<field name="excelData" type="imageFile" maxFileSize="2147483647" />
	</fields>
</DataSource>