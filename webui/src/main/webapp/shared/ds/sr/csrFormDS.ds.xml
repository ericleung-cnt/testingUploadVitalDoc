<DataSource ID="csrFormDS" serverType="generic"
	inheritsFrom="abstractPersistentEntityDS">
	<serverObject lookupStyle="spring" bean="csrFormDMI" />
	<fields>
		<field name="imoNo" type="text" length="9" primaryKey="true" required="true" title="IMO No."/>
