<DataSource ID="preReserveDS" serverType="generic" inheritsFrom="abstractPersistentEntityDS">
	<serverObject lookupStyle="spring" bean="preReserveDMI" />
	<fields>
		<field name="id" 		type="integer" title="ID" 		length="20" 	primaryKey="true" required="true"/>
		<field name="name1" 	type="text" title="Name 1" 	length="30" required="true"/>
		<field name="name2" 	type="text" title="Name 2" 	length="30"/>
		<field name="name3" 	type="text" title="Name 3" 	length="30"/>
		<field name="chName1" 	type="text" title="Chi Name 1" 	length="30"/>
		<field name="chName2" 	type="text" title="Chi Name 2" 	length="30"/>
		<field name="chName3" 	type="text" title="Chi Name 3" 	length="30"/>
		<field name="entryTime" 	type="date" displayFormat="dd/MM/yyyy" title="Entry Date" 	/>

		<field name="applicant" type="text" title="Applicant Name" length="65" required="true"/>
		<field name="addr1" type="text" title="Address 1" length="80" />
		<field name="addr2" type="text" title="Address 2" length="80" />
		<field name="addr3" type="text" title="Address 3" length="80" />
		<field name="tel" type="text" title="Tel. No." length="18" />
		<field name="fax" type="text" title="Fax No." length="18" />
		<field name="email" type="text" title="Email" length="30" />
		<field name="owner" type="text" title="Owner Name" length="65" required="true"/>

		<field name="name" 	type="text" title="Name" 	length="30"/>
		<field name="chName" 	type="text" title="Chi Name" 	length="30"/>
	</fields>
</DataSource>